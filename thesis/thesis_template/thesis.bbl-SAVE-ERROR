      \strng{namehash}{85437fe07696e4c57f916c83d4ba5581}
      \strng{fullhash}{28676992274ff1a4ebfa6e23ba7b2efd}
      \strng{fullhashraw}{28676992274ff1a4ebfa6e23ba7b2efd}
      \strng{bibnamehash}{85437fe07696e4c57f916c83d4ba5581}
